 <head>
<title>Bootstrap 101 Template</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<!-- Bootstrap -->
<link href="css/bootstrap.min.css" rel="stylesheet" media="screen">
</head>

<table>
  <tr>
    <th>Name</th>
    <th>Location</th>
    <th>Orientation</th>
    <th>Running</th>
    <th>shots</th>
  </tr>
 
<% @officers.each do |o| %>
  <tr>
    <td><%= o.name %></td>
    <td><%= o.location %></td>
    <td>"<iframe width="425" height="350" frameborder="0"
    scrolling="no" marginheight="0" marginwidth="0"
    src="https://maps.google.com/maps?q=<%= o.location
    %>&amp;t=h&amp;ie=UTF8&amp;z=14&amp;ll=<%= o.location
    %>&amp;output=embed"></iframe><br/><small><a href="https://maps.google.com/maps?q=<%= o.location
    %>&amp;t=h&amp;ie=UTF8&amp;z=14&amp;ll=<%= o.location
    %>&amp;source=embed" style="color:#0000FF;text-align:left">View
    Larger Map</a></small>"></td>
    <td><%= o.orientation %></td>
    <td><%= o.running %></td>
    <td><%= o.shots %></td>
  </tr>
<% end %>
</table>
 
<br />
<script src="http://code.jquery.com/jquery.js"></script>
<script src="js/bootstrap.min.js"></script>
